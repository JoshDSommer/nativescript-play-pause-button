{
  "name": "nativescript-play-pause-button",
  "version": "0.1.1",
  "description": "NativeScript Play Pause Button",
  "main": "play-pause-button.js",
  "typings": "play-pause-button.d.ts",
  "nativescript": {
    "platforms": {
      "android": "2.1.1",
      "ios": "2.1.1"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo && cd demo && tns emulate ios",
    "demo.android": "npm run preparedemo && cd demo && tns run android",
    "preparedemo": "npm run build && cd demo && tns plugin remove nativescript-play-pause-button && tns plugin add .. && tns install",
    "setup": "cd demo && npm install && cd .. && npm run build && cd demo && tns plugin add .. && cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YourName/nativescript-play-pause-button.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS"
  ],
  "author": {
    "name": "Josh Sommer",
    "email": "JoshDSommer@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/TheOriginalJosh/nativescript-play-pause-button/issues"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/TheOriginalJosh/nativescript-play-pause-button/blob/master/LICENSE"
  },
  "homepage": "https://github.com/TheOriginalJosh/nativescript-play-pause-button",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tns-platform-declarations": "^2.0.0",
    "tns-core-modules": "next",
    "typescript": "^1.8.10"
  }
}
